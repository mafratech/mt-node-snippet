      "  async index(request, response) { }",
      "\t",
      "  async show(request, response) { }",
      "\t",
      "  async store(request, response) { }",
      "\t",
      "  async update(request, response) { }",
      "\t",
      "  async delete(request, response) { }",
      "}",
      "\t",
      "export default new ${0:${TM_FILENAME/(.*)\\..+$/$1/}Controller}()"
